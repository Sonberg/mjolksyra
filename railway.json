{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "buildContext": "mjolksyra-api",
    "dockerfilePath": "Mjolksyra.Api/Dockerfile"
  },
  "deploy": {
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10,
    "healthcheckPath": "/openapi/v1.json",
    "healthcheckTimeout": 120
  }
}
